#include <iostream>
using namespace std;
class Node{
    public:
        int data;
        Node* prev;
        Node* next;
};
class LRU{
    public:
        Node* head;
        Node* rear;
        LRU(){
            head=rear=NULL;
        }

        bool empty(){
            return head == NULL;
        }

        void push(int x){
            Node* n = new Node();
            n->data = x;
            n->next = NULL;
            n->prev = rear;

            if (empty()) {
                head = rear = n;
            }
            else{
                rear->next = n;
                rear = n;
            }
        }

        void pop(){
            if (empty()){
                cout << "Deque is empty\n";
                return;
            }

            Node* temp = head;

            if (head == rear){
                head = rear = NULL;
            }
            else{
                head = head->next;
                head->prev = NULL;
            }

            delete temp;
        }

        bool find(Node* head,int val){
            Node* temp=head;
            while(temp!=NULL){
                if(temp->data==val) return true;
                temp=temp->next;
            }
            return false;
        }


};
int main(){

}